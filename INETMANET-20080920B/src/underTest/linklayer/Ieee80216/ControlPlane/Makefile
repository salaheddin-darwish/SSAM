generateheaders: $(GENERATEDHEADERS)
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) generateheaders) || exit 1; done

clean:
	rm -f *.o *_n.cc *_n.h *_m.cc *_m.h .tstamp
	rm -f *.vec *.sca
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) clean); done

depend:
	$(MAKEDEPEND) $(INCLUDE_PATH) -- *.cc
	# $(MAKEDEPEND) $(INCLUDE_PATH) -fMakefile.in -- *.cc
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) depend) || exit 1; done

makefiles:
	# recreate Makefile
	opp_makemake -f  -N -b /home/siedlaczek/INET -c /home/siedlaczek/INET/inetconfig -I. -n -I/home/siedlaczek/INET/Transport/Contract -I/home/siedlaczek/INET/Network/Contract -I/home/siedlaczek/INET/NetworkInterfaces/Contract -I/home/siedlaczek/INET/Base -I/home/siedlaczek/INET/Util -I../MACSublayer -I../../Ethernett 
